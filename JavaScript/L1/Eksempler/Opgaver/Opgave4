let l1 = [5, 6, 7, 8, 9]
let l2 = [1, 2, 3, 4, 5]
let i1 = 0;
let i2 = 0;
let result = [];

while(i1 < l1.length && i2 < l2.length){
    if(l1[i1] <= l2[i2]){
        result.push(l1[i1]);
        i1++;
    } else {
        result.push(l2[i2]);
        i2++;
    }
}
while(i1 < l1.length){
    result.push(l1[i1]);
    i1++;
}
while(i2 < l2.length){
    result.push(l2[i2]);
    i2++;
}
console.log(result)